CREATE TABLE TBL_USER(
USER_NUMBER NUMBER,
USER_NAME VARCHAR2(100),
USER_AGE NUMBER,
USER_PHONENUMBER VARCHAR2(1000),
USER_ADDRES VARCHAR2(1000),
CONSTRAINT PK_USER PRIMARY KEY(USER_NUMBER)
);

CREATE TABLE TBL_BOOK(
BOOK_NUMBER NUMBER,
BOOK_NAME VARCHAR2(100),
BOOK_GENRE VARCHAR2(100),
BOOK_USER_NUMBER NUMBER,
CONSTRAINT PK_BOOK PRIMARY KEY(BOOK_NUMBER),
CONSTRAINT FK_BOOK FOREIGN KEY(BOOK_USER_NUMBER) REFERENCES TBL_USER(USER_NUMBER)
);